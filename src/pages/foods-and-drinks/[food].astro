---
import DetailsLayout from "@layouts/DetailsLayout.astro";
import { getCollection } from "astro:content";

export async function getStaticPaths() {
  const foodsCollection = await getCollection("foods-and-drinks");

  return foodsCollection.map((food) => {
    return {
      params: {
        food: food.slug,
      },
      props: {
        food,
      },
    };
  });
}

const { food } = Astro.props;
---

<DetailsLayout place={food} />
